package acs;

// P6: The geometry of buildings serves to define which buildings can
// communicate between each other, and in which direction.
class Building {	
  ref Building[*] #building gate;
  ref Building[*] #gate building;
}

class Person {
  op void pass(Building b);
  ref Building[1] sit;
  ref Boolean blub;
  ref Building[*] aut;  
}
